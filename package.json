{
  "dependencies": {
    "next": "12.1.6",
    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@types/node": "18.11.17",
    "@types/react": "18.0.9",
    "eslint": "8.16.0",
    "eslint-config-next": "12.1.6",
    "husky": "8.0.1",
    "lint-staged": "12.4.2",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.2"
  },
  "engines": {
    "node": ">=18.2.0 <19.0.0",
    "npm": ">=8.11.0 <9.0.0"
  },
  "name": "nextjs-boilerplate",
  "packageManager": "npm@8.11.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "fix": "run-s fix:eslint fix:prettier",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "lint": "run-p lint:*",
    "lint:eslint": "next lint",
    "lint:prettier": "prettier --check --ignore-path .gitignore .",
    "prepare": "husky install"
  },
  "version": "0.1.0"
}
